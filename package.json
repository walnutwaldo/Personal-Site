{
  "name": "personal_site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^5.1.1",
    "feather-icons-react": "^0.6.2",
    "firebase": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-rc.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "react-social-icons": "^5.15.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "build-sass": "sass src/index.scss src/css/index.css",
    "build-react": "react-scripts build",
    "build": "npm-run-all -s build-sass build-react",
    "start-sass": "sass --watch src/index.scss:src/css/index.css",
    "start-react": "react-scripts start",
    "start": "npm-run-all -p start-sass start-react",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "yarn prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "2.8.7",
    "sass": "^1.42.1"
  }
}
